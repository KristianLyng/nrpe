# Source code directories
SRC_BASE=./src/
SRC_COMMON=./common/

all:
	cd $(SRC_BASE); $(MAKE) ; cd ..

nrpe:
	cd $(SRC_BASE); $(MAKE) ; cd ..

check_nrpe:
	cd $(SRC_BASE); $(MAKE) ; cd ..


clean:
	cd $(SRC_BASE); $(MAKE) $@ ; cd ..
	rm -f core
	rm -f *~ */*~
	rm -f config.log config.status config.cache Makefile $(SRC_COMMON)/config.h

distclean: clean

devclean: clean

